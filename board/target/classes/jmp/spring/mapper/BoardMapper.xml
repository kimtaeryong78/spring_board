<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jmp.spring.mapper.BoardMapper">
	<!-- get tbl_board table List -->
	<select id="getList" resultType="jmp.spring.vo.BoardVO">
		select * from tbl_board order BY bno DESC
	</select>
	<insert id="insertBoard">
		insert into tbl_board (bno, title, content, writer, regdate, updatedate)
		values (seq_board.nextval, #{title}, #{content}, #{writer}, sysdate, sysdate)
	</insert>
	<!-- <select id="getBoard" resultType="jmp.spring.vo.BoardVO"> select * 
		from tbl_board where bno = #{bno} </select> -->
	<!-- <update id="updateBoard"> update tbl_board set title = #{title}, content 
		= #{content}, updatedate = sysdate where bno = #{bno} </update> <delete id="deleteBoard"> 
		delete from tbl_board where bno = #{bno} </delete> -->
</mapper>